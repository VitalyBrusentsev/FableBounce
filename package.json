{
  "name": "fablebounce",
  "version": "1.0.0",
  "description": "Fable canvas toy project",
  "scripts": {
    "postinstall": "dotnet tool restore",
    "start": "dotnet fable watch src -s --sourceMapsRoot src --run webpack serve --mode development",
    "build": "dotnet fable ./ && webpack --mode production --node-env production"
  },
  "devDependencies": {
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "author": "Vitaly Brusentsev",
  "license": "ISC"
}
