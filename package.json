{
  "name": "fablebounce",
  "version": "1.0.0",
  "description": "Fable canvas toy project",
  "scripts": {
    "postinstall": "dotnet tool restore",
    "start": "dotnet fable watch src/App.fsproj -s --sourceMapsRoot src --run webpack serve --mode development",
    "build": "dotnet fable src/App.fsproj && webpack --mode production --node-env production"
  },
  "devDependencies": {
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "overrides": {
    "body-parser": "^1.20.3",
    "brace-expansion": "^1.1.12",
    "compression": "^1.7.5",
    "cookie": "^0.7.0",
    "cross-spawn": "^7.0.5",
    "express": "^4.22.0",
    "http-proxy-middleware": "^2.0.7",
    "micromatch": "^4.0.8",
    "node-forge": "^1.3.2",
    "on-headers": "^1.0.2",
    "path-to-regexp": "^0.1.12",
    "send": "^0.19.0",
    "serve-static": "^1.16.2"
  },
  "author": "Vitaly Brusentsev",
  "license": "ISC"
}
